{
  "name": "@codewithkyle/jsql",
  "version": "1.0.13",
  "description": "",
  "main": "jsql.js",
  "types": "jsql.d.ts",
  "files": [
    "jsql.js",
    "jsql.worker.js",
    "jsql.d.ts",
    "stream.worker.js"
  ],
  "scripts": {
    "bundle": "node ./build/bundle.js",
    "build": "esbuild ./src/jsql.ts --outfile=jsql.js --format=esm --minify && esbuild ./src/jsql.worker.ts --outfile=jsql.worker.js --minify --bundle && esbuild ./src/stream-parser.worker.ts --outfile=stream.worker.js --minify --bundle",
    "build:test": "npm run build && cp ./jsql.js ./test && cp ./jsql.worker.js ./test && cp ./stream.worker.js ./test",
    "test": "serve ./test"
  },
  "author": "Kyle Andrews <codingwithkyle@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "esbuild": "^0.11.23",
    "esinstall": "^1.1.7",
    "fuse.js": "^6.4.6",
    "idb": "^6.1.2",
    "serve": "^11.3.2"
  },
  "bundle": [
    "idb",
    "fuse.js"
  ]
}
